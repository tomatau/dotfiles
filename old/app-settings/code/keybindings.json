[
  {
    "key": "shift+cmd+d",
    "command": "geeebe.duplicateText",
    "when": "editorTextFocus"
  },
  {
    "key": "cmd+d",
    "command": "editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "cmd+k cmd+right",
    "command": "workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "shift+ctrl+/",
    "command": "editor.action.blockComment"
  },
  // damn thing breaks minimize, override to remove the maths crap
  {
    "key": "cmd+m",
    "command": "",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+shift+\\",
    "command": "workbench.action.toggleAuxiliaryBar"
  },
  {
    "key": "ctrl+shift+t",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "cmd+j",
    "command": "-workbench.action.togglePanel"
  },
  {
    "key": "ctrl+shift+c",
    "command": "workbench.files.action.collapseExplorerFolders"
  },
  {
    "key": "shift+cmd+f",
    "command": "-markdown-table.convert.split",
    "when": "editorTextFocus"
  },
  {
    "key": "left",
    "command": "-settings.action.focusTOC",
    "when": "inSettingsEditor && settingRowFocus"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "-jupyter.runAndDebugCell",
    "when": "isWorkspaceTrusted && jupyter.ispythonnotebook && !notebookKernel && notebookCellType == 'code' || isWorkspaceTrusted && jupyter.ispythonnotebook && notebookCellType == 'code' && notebookKernel =~ /^ms-toolsai.jupyter\\// && resource not in 'jupyter.notebookeditor.debugDocuments'"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "workbench.action.toggleMaximizeEditorGroup",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
  },
  {
    "key": "cmd+k cmd+m",
    "command": "-workbench.action.toggleMaximizeEditorGroup",
    "when": "editorPartMaximizedEditorGroup || editorPartMultipleEditorGroups"
  }
]

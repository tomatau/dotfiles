{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "OverpassM Nerd Font Propo, Operator Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.rulers": [80, 100],
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 2,
  "editor.semanticHighlighting.enabled": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      /* COMMON */
      {
        "scope": "punctuation.definition.comment",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#ff7bed"
        }
      },
      {
        "scope": "comment, comment.line.double-slash",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#ff7bed"
        }
      },
      {
        "scope": "heading.2.markdown entity.name.section.markdown",
        "settings": {
          "foreground": "#ff9421"
        }
      },
      {
        "scope": "heading.3.markdown entity.name.section.markdown",
        "settings": {
          "foreground": "#83d440"
        }
      },
      {
        "scope": "heading.4.markdown entity.name.section.markdown",
        "settings": {
          "foreground": "#00d5ff"
        }
      },
      {
        "scope": "markup.quote",
        "settings": {
          "foreground": "#60a66f"
        }
      }
    ],
    "[Palenight Operator]": {
      "textMateRules": [
        {
          "scope": "entity.name.function",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#6ed265"
          }
        },
        {
          "scope": "punctuation.separator",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#6ed265"
          }
        },
        {
          "scope": "string.quoted",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#e1d9a2"
          }
        },
        /* PYTHON */
        {
          "scope": "support.type, entity.name.function.decorator.python, meta.function.decorator.python",
          "settings": {
            "foreground": "#68b8ea"
          }
        },
        {
          "scope": "meta.function-call.generic.python",
          "settings": {
            "foreground": "#FFCB6B"
          }
        },
        {
          "scope": "support.type.exception.python",
          "settings": {
            "foreground": "#FF5874"
          }
        },
        {
          "scope": "variable.parameter.function.language.python",
          "settings": {
            "foreground": "#f4fbff"
          }
        },
        {
          "scope": "variable.language.special.self.python, variable.parameter.function.language.special.self.python",
          "settings": {
            "foreground": "#f8a97c"
          }
        },
        {
          "scope": "source.python",
          "settings": {
            "foreground": "#e2f4fe"
          }
        },
        {
          "scope": "support.function.magic.python",
          "settings": {
            "foreground": "#ccd568"
          }
        },
        {
          "scope": "variable.parameter.function-call.python",
          "settings": {
            "foreground": "#fcab72"
          }
        },
        {
          "scope": "meta.function-call.arguments.python, variable.language.special",
          "settings": {
            "foreground": "#e3eeff"
          }
        },
        {
          "scope": "meta.attribute.python, meta.member.access.python",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#a5d598"
          }
        },
        {
          "scope": "string.quoted.docstring.multi.python",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#ff7bed"
          }
        }
      ]
    }
  },
  "editor.wordWrap": "on",
  "editor.wrappingIndent": "deepIndent",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "emmet.syntaxProfiles": {
    "js": {
      "attr_quotes": "single"
    },
    "jsx": {
      "attr_quotes": "single"
    },
    "ts": {
      "attr_quotes": "single"
    },
    "tsx": {
      "attr_quotes": "single"
    }
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 1,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "**/.github/**/*.yml": "github-actions-workflow",
    "*.mdx": "mdx",
    "Modelfile": "modelfile"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.allowForcePush": true,
  "git.confirmForcePush": false,
  "git.confirmSync": false,
  "git.rebaseWhenSync": true,
  "grammarly.files.include": [
    "**/readme.md",
    "**/README.md",
    "**/*.txt",
    "**/*.mdx"
  ],
  "grammarly.selectors": [
    {
      "language": "markdown",
      "scheme": "file"
    }
  ],
  "html.format.wrapLineLength": 90,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jupyter.interactiveWindow.creationMode": "perFile",
  "liveServer.settings.donotShowInfoMsg": true,
  "npm.enableRunFromFolder": true,
  "prettier.arrowParens": "avoid",
  "prettier.bracketSameLine": false,
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.validate.enable": true,
  "window.restoreWindows": "all",
  "workbench.colorCustomizations": {
    "editorCodeLens.foreground": "#787776e6",
    "editorCursor.foreground": "#00ff40",
    "notebook.cellBorderColor": "#111",
    "notebook.editorBackground": "#232629",
    "notebook.focusedCellBackground": "#232629",
    "notebook.focusedCellBorder": "#ff9500",
    "notebook.outputContainerBackgroundColor": "#16181a",
    "notebook.selectedCellBackground": "#232629",
    "[Palenight Operator]": {
      "statusBar.background": "#1c202f",
      "activityBar.background": "#1c202f",
      "activityBar.border": "#444c6c",
      "sideBar.border": "#222637",
      "sideBar.background": "#222637",
      "sideBarSectionHeader.background": "#222637",
      "editorGroup.border": "#677094",
      "panel.border": "#484e66"
    }
  },
  "workbench.colorTheme": "Palenight Operator",
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.enableExperiments": false,
  "workbench.startupEditor": "newUntitledFile",
  "write-good.languages": ["markdown", "md", "mdx", "plaintext"],
  "terminal.integrated.inheritEnv": false,
  "grammarly.config.documentDialect": "british",
  "grammarly.config.documentDomain": "casual",
  "window.zoomLevel": 1,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "notebook.stickyScroll.enabled": true,
  "editor.stickyScroll.enabled": true,
  "workbench.tree.stickyScrollMaxItemCount": 5,
  "[rust]": {
    "editor.defaultFormatter": "jinxdash.prettier-rust"
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "explorer.confirmPasteNative": false,
  "gitlens.views.commitDetails.files.layout": "tree",
  "cSpell.userWords": ["pulumi", "rustacean"],
  "jupyter.interactiveWindow.textEditor.executeSelection": true,
  "window.customTitleBarVisibility": "windowed"
}
